    w.document.writeln ('<input type=hidden name=file value="'+file+'">'); 
    w.document.writeln ('<p><textarea cols=72 rows=8 name=description autowrap>');
    w.document.writeln (description);
    w.document.writeln ('</textarea></p>'); 
    w.document.writeln ('<p>Password: <input type=password name=pass size=8></p>'); 
    w.document.writeln ('<input type=submit value="Enter Description"></form></body></html>');
    w.document.close();
    return;
    }
    
//-->
</script>

