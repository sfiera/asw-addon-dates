function rmmvDlog (cleanfile, product, category, file) {
    var winProps = 'toolbar=0,location=0,directories=0,menubar=0,width=300,height=120';
    var winName = ('rmmvDlog');
    var w = window.open ('', winName, winProps);
    w.document.close();
    w.document.writeln ('<head><title>Move or Delete "'+cleanfile+'"</title></head><body bgcolor=#000000 link=#BFBFFF alink=#BFBFFF vlink=#FFFFFF text=#FFFFFF background="">');
    w.document.writeln ('<font face="Geneva, Verdana, Arial" size="1"><b>Move or Delete "'+cleanfile+'"</b>');
    w.document.writeln ('<form action="http://www.ambrosiasw.com/cgi-bin/vftp/rm-mv.pl" method=post>');
    w.document.writeln ('<input type=hidden name=product value="'+product+'">'); 
    w.document.writeln ('<input type=hidden name=category value="'+category+'">'); 
    w.document.writeln ('<input type=hidden name=file value="'+file+'">'); 
    w.document.writeln ('<p>Password: <input type=password name=pass size=8></p>'); 
    w.document.writeln ('<select name=target><option value=remove>Delete'); 
    w.document.writeln ('<option value=addons>addons');
    w.document.writeln ('<option value=essentials>essentials');
    w.document.writeln ('<option value=guides>guides');
    w.document.writeln ('<option value=levels>levels');
    w.document.writeln ('</select>'); 
    w.document.writeln ('<input type=submit value="Do It"></form></body></html>');
    w.document.close();
    return;
    }

function rateDlog (cleanfile, product, category, file, admin) {
    var winProps = 'toolbar=0,location=0,directories=0,menubar=0,width=220,height=120';
    var winName = ('rateDlog');
    var w = window.open ('', winName, winProps);
    w.document.close();
    w.document.writeln ('<head><title>Rate "'+cleanfile+'"</title></head><body bgcolor=#000000 link=#BFBFFF alink=#BFBFFF vlink=#FFFFFF text=#FFFFFF background="">');
    w.document.writeln ('<font face="Geneva, Verdana, Arial" size="1"><b>Rate "'+cleanfile+'"</b>');
    w.document.writeln ('<form action="http://www.ambrosiasw.com/cgi-bin/vftp/rating.pl" method=post>');
    w.document.writeln ('<input type=hidden name=product value="'+product+'">'); 
    w.document.writeln ('<input type=hidden name=category value="'+category+'">'); 
    w.document.writeln ('<input type=hidden name=file value="'+file+'">'); 
    w.document.writeln ('<p><select name=vote>'); 
    w.document.writeln ('<option value="">Choose a Value<option value=5>5 (Excellent)<option value=4>4<option value=3>3<option value=2>2<option value=1>1 (Terrible)');
    if (admin) { 
      w.document.writeln ('<option value="reset">Reset'); 
      }
    w.document.writeln ('</select><input type=submit value="Rate It"></p>');
    if (admin) { 
      w.document.writeln ('Password for Reset: <input type=password name=pass size=8>'); 
      }
    w.document.writeln ('</form></body></html>'); 
    w.document.close();
    return;
    }

